<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CuartaActividad">

    <!--Izquierda Arriba-->
    <Button
        android:id="@+id/buttonInicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="200dp"
        android:text="Inicio"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.482"
        app:layout_constraintStart_toStartOf="parent" />

    <FrameLayout
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="#379292"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_gravity="left"
        app:layout_constraintTop_toTopOf="parent" />

    <!--Centro Arriba-->
    <FrameLayout
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="#40B35E"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!--Derecha Arriba-->
    <FrameLayout
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="#4CED74"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="16dp"/>


    <!--Izquierda Centro-->
    <FrameLayout
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="#814FBD"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_gravity="left"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <!--Centro Centro-->
    <FrameLayout
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="#00428E"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />

    <!--Derecha centro-->
    <FrameLayout
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="#149DFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="16dp"
        app:layout_constraintBottom_toBottomOf="parent" />

    <!--Izquierda Abajo-->
    <FrameLayout
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="#FF2E66"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_gravity="left"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"/>

    <!--Centro Abajo-->
    <FrameLayout
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="#FF0044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginBottom="16dp" />

    <!--Derecha Abajo-->
    <FrameLayout
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="#FFA8A8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="16dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>